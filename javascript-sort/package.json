{
  "name": "javascript-sort",
  "displayName": "Javascript Sort",
  "description": "Deeply sort the keys within a javascript object",
  "version": "0.0.1",
  "publisher": "gaz77a",
  "author": {
    "name": "gaz77a"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com:gaz77a/vscode-extensions.git"
  },
  "icon": "resources/javaScript-sort-256.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.javaScript-sort"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.javaScript-sort",
        "title": "JS Sort"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "unit-test": "mocha -r ts-node/register 'src/**/*.unittest.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.6",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
